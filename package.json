{
  "name": "dot-map-renderer",
  "version": "0.0.3",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "terser": "^3.14.1",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "workspaces-run": "^1.0.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "map",
    "dot",
    "world"
  ],
  "author": "moretall",
  "scripts": {
    "start:dev": "webpack serve",
    "build": "webpack",
    "publish": "npm publish --access=public",
    "publish:all": "npm publish -w ./packages/",
    "types": "tsc bundles/src/index.ts --declaration --emitDeclarationOnly --resolveJsonModule --outdir ./dist --esModuleInterop",
    "patch:all": "npm version patch -w ./packages/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true
}
